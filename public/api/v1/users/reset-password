<!doctype html>
<html>
  <head>
    <meta charset="utf-8" />
    <meta http-equiv="refresh" content="0;url=/reset-password" />
    <script>
      // Preserve query string (e.g., ?token=...)
      (function(){
        var qs = window.location.search || '';
        window.location.replace('/reset-password' + qs);
      })();
    </script>
  </head>
  <body></body>
  </html>


